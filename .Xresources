!~ URxvt Settings ~!
URxvt.saveLines:512
!URxvt*font:         xft:SF Mono:style=SemiBold:size=8
!URxvt.boldfont:     xft:SF Mono:style=Bold:size=8
!URxvt*font:        xft:JetBrains Mono-9
!URxvt.boldfont:    xft:JetBrains Mono Medium-9
URxvt*font:        xft:cherry:size=9
URxvt.boldfont:    xft:cherry:size=9
!URxvt*font:        xft:AcPlus IBM VGA 8x14:style=Regula:size=12
!URxvt.boldfont:    xft:AcPlus IBM VGA 8x14:style=Regula:size=12


URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1

Xft.dpi:                      108
Xft.antialias:                  1
Xft.autohint:                   1
Xft.bold:                       0
Xft.hinting:                    1
Xft.hintstyle:                 full
Xft.lcdfilter:                 full
Xft.rgba:                       rgb

URxvt.lineSpace:                0
URxvt.letterSpace:              0

URxvt.skipScroll:               true
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt.scrollBar:                false
URxvt.loginShell:               true
URxvt.internalBorder:	        15
URxvt.perl-ext:	 		default,matcher
URxvt.url-launcher:            qutebrowser
URxvt*fading:  0
URxvt*blurRadius:  0
URxvt*depth:  0
URxvt*inheritPixmap: false
URxvt.transparent: false
URxvt.shading: 35
URxvt.Geometry: 120x120
URxvt.matcher.button: 1

