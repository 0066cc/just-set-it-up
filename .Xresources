!~ URxvt Settings ~!
URxvt.saveLines:512
!URxvt*font:         xft:JetBrains Mono-9
!URxvt.boldfont:     xft:JetBrains Mono Medium-9

URxvt*font:         xft:SF Mono:style=SemiBold:size=8
URxvt.boldfont:     xft:SF Mono:style=Bold:size=8

URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1

Xft.dpi:                      108
Xft.antialias:                  1
Xft.autohint:                   1
Xft.bold:                       1
Xft.hinting:                    1
Xft.hintstyle:                 full
Xft.lcdfilter:                 full
Xft.rgba:                       rgb

URxvt.lineSpace:                -1
URxvt.letterSpace:             0

URxvt.skipScroll:               true
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt.scrollBar:                false
URxvt.loginShell:               true
URxvt.internalBorder:	        45
URxvt.perl-ext:	 		default,matcher
URxvt.url-launcher:            google-chrome
URxvt*fading:  0
URxvt*blurRadius:  0
URxvt*depth:  0
URxvt*inheritPixmap: false
URxvt.transparent: false
URxvt.shading: 40
URxvt.Geometry: 120x120
URxvt.matcher.button: 1

! Special
*background:    #1e2832
*foreground:    #caf0ec
*.cursorColor:  #FFFFFF

! Black
*color0: #353e47
*color8: #59626b

! Red
*color1: #675665
*color9: #927a90

! Green
*color2: #3a6469
*color10: #5a9295

! Yellow
*color3: #a5b0aa
*color11: #e1f0e4

! Blue
*color4: #515e71
*color12: #6e7f99

! Magenta
*color5:  #636d73
*color13: #81779d

! Cyan
*color6: #2b908c
*color14: #3bc7bc

! White
*color7:  #bdb8c1
*color15: #ebcbde

! Rofi
rofi.color-normal: argb:00000000, #515e71, argb:00000000, #1e2832,#2b908c
rofi.color-urgent: argb:00000000, #00ff00, argb:00000000, #110000,#fae1ee
rofi.color-active: argb:00000000, #0000ff, argb:00000000, #000011,#caf0ec
rofi.color-window: argb:ff1e2832, #515e71, #3bc7bc
