Xft.dpi:                      108
Xft.antialias:                  1
Xft.autohint:                   1
Xft.bold:                       0
Xft.hinting:                    1
Xft.hintstyle:                 full
Xft.lcdfilter:                 full
Xft.rgba:                       rgb

!~ URxvt Settings ~!
URxvt.saveLines:512
URxvt*font:        xft:JetBrains Mono Medium-9
URxvt.boldfont:    xft:JetBrains Mono Medium-9
!URxvt*font:        xft:iA Writer Mono S:size=9
!URxvt.boldfont:    xft:iA Writer Mono S:size=9:weight=bold

URxvt.lineSpace:                0
URxvt.letterSpace:              0
URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1
URxvt.skipScroll:               true
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt.scrollBar:                false
URxvt.loginShell:               true
URxvt.internalBorder:	        15
URxvt.perl-ext:	 		default,matcher
URxvt.url-launcher:           microsoft-edge 
URxvt*fading:  0
URxvt*blurRadius:  0
URxvt*depth:  0
URxvt*inheritPixmap: true
URxvt.transparent: true
URxvt.shading: 15
URxvt.Geometry: 120x120
URxvt.matcher.button: 1

*background: #1d2021
