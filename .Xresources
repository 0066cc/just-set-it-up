!~ URxvt Settings ~!
URxvt.saveLines:512
URxvt*font:         xft:SF Mono:style=SemiBold:size=8
URxvt.boldfont:     xft:SF Mono:style=Bold:size=8
!URxvt*font:        xft:JetBrains Mono-9
!URxvt.boldfont:    xft:JetBrains Mono Medium-9

URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1

Xft.dpi:                      108
Xft.antialias:                  1
Xft.autohint:                   1
Xft.bold:                       0
Xft.hinting:                    1
Xft.hintstyle:                 full
Xft.lcdfilter:                 full
Xft.rgba:                       rgb

URxvt.lineSpace:                0
URxvt.letterSpace:              -1

URxvt.skipScroll:               true
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt.scrollBar:                false
URxvt.loginShell:               true
URxvt.internalBorder:	        15
URxvt.perl-ext:	 		default,matcher
URxvt.url-launcher:            google-chrome
URxvt*fading:  0
URxvt*blurRadius:  0
URxvt*depth:  0
URxvt*inheritPixmap: true
URxvt.transparent: true
URxvt.shading: 35
URxvt.Geometry: 120x120
URxvt.matcher.button: 1

*.foreground:   #c0c0c0
*.background:   #121212
*.cursorColor:  #c0c0c0

! black
*.color0:       #080808
*.color8:       #36393f

! red
*.color1:       #515978
*.color9:       #778099

! green
*.color2:       #3ea28d
*.color10:      #71c4ba

! yellow
*.color3:       #4dc5ce
*.color11:      #5e767f

! blue
*.color4:       #6ea2a5
*.color12:      #6abdc1

! magenta
*.color5:       #a8a8a8
*.color13:      #4e4d80

! cyan
*.color6:       #558188
*.color14:      #4fc8ce

! white
