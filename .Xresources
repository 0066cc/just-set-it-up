!~ URxvt Settings ~!
URxvt.saveLines:512
!URxvt*font:         xft:SF Mono:style=SemiBold:size=8
!URxvt.boldfont:     xft:SF Mono:style=Bold:size=8
URxvt*font:        xft:JetBrains Mono-9
URxvt.boldfont:    xft:JetBrains Mono Medium-9

URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1

Xft.dpi:                      108
Xft.antialias:                  1
Xft.autohint:                   1
Xft.bold:                       0
Xft.hinting:                    1
Xft.hintstyle:                 full
Xft.lcdfilter:                 full
Xft.rgba:                       rgb

URxvt.lineSpace:                -1
URxvt.letterSpace:              -1

URxvt.skipScroll:               true
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt.scrollBar:                false
URxvt.loginShell:               true
URxvt.internalBorder:	        15
URxvt.perl-ext:	 		default,matcher
URxvt.url-launcher:            google-chrome
URxvt*fading:  0
URxvt*blurRadius:  0
URxvt*depth:  0
URxvt*inheritPixmap: false
URxvt.transparent: false
URxvt.shading: 35
URxvt.Geometry: 120x120
URxvt.matcher.button: 1

*.foreground:  #7c8fa4
*.background:  #0e1011
*.cursorColor: #708284
