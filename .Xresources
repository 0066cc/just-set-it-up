!~ URxvt Settings ~!
URxvt.saveLines:512

URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1
URxvt*font:          xft:Cherry-8

Xft.dpi:                      108
Xft.antialias:        		1
Xft.autohint:                   1
Xft.hinting:                    1
Xft.hintstyle:                 full
Xft.lcdfilter:                 full
Xft.rgba:                       rgb

URxvt.lineSpace:                -1
URxvt.letterSpace:              0
URxvt.skipScroll:               true
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt.scrollBar:                false
URxvt.loginShell:               true
URxvt.internalBorder:	25
URxvt.perl-ext:	 		default,matcher
URxvt.url-launcher:            google-chrome
URxvt*fading:  0
URxvt.transparent: false
URxvt.shading: 20
URxvt.Geometry: 120x96
URxvt.tintColor: #ffffff
URxvt.matcher.button: 		1


