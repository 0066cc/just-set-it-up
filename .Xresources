!~ URxvt Settings ~!
URxvt.saveLines:512
!URxvt*font:         xft:JetBrains Mono-9
!URxvt.boldfont:     xft:JetBrains Mono Medium-9

URxvt*font:         xft:SF Mono:style=SemiBold:size=8
URxvt.boldfont:     xft:SF Mono:style=Bold:size=8

URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1

Xft.dpi:                      108
Xft.antialias:                  1
Xft.autohint:                   1
Xft.bold:                       1
Xft.hinting:                    1
Xft.hintstyle:                 full
Xft.lcdfilter:                 full
Xft.rgba:                       rgb

URxvt.lineSpace:                -1
URxvt.letterSpace:             0

URxvt.skipScroll:               true
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt.scrollBar:                false
URxvt.loginShell:               true
URxvt.internalBorder:	        45
URxvt.perl-ext:	 		default,matcher
URxvt.url-launcher:            google-chrome
URxvt*fading:  0
URxvt*blurRadius:  0
URxvt*depth:  0
URxvt*inheritPixmap: false
URxvt.transparent: false
URxvt.shading: 190
URxvt.Geometry: 120x120
URxvt.matcher.button: 1

*.foreground:   #ffffff
*.background:   #262934
*.cursorColor:  #ffffff

! black
*.color0:       #000000
*.color8:       #555753

! red
*.color1:       #fb4343
*.color9:       #fc7979

! green
*.color2:       #4e9a06
*.color10:      #8fe811

! yellow
*.color3:       #c4a000
*.color11:      #fce94f

! blue
*.color4:       #afd2ff
*.color12:      #7baee3

! magenta
*.color5:       #85678f
