!~ URxvt Settings ~!
URxvt.saveLines:512

URxvt.cursorBlink: 0
URxvt.cursorUnderline: 0
URxvt.boldFont:
!URxvt*font:          xft:dweep-8
URxvt*font:          xft:Perfect DOS VGA 437-9
!URxvt*font:          xft:TI-99/4A-12
!URxvt*font:          xft:Sinclair ZX Spectrum ES-6
!URxvt*font:          xft:JetBrain Mono-8

Xft.dpi:                      108
Xft.antialias:        		1
Xft.autohint:                   1
Xft.hinting:                    1
Xft.hintstyle:                 full
Xft.lcdfilter:                 full
Xft.rgba:                       rgb

URxvt.lineSpace:                -1
URxvt.letterSpace:              -1
URxvt.skipScroll:               true
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt.scrollBar:                false
URxvt.loginShell:               true
URxvt.internalBorder:	25
URxvt.perl-ext:	 		default,matcher
URxvt.url-launcher:            google-chrome
URxvt*fading:  50
URxvt*blurRadius:  0
URxvt*depth:  32
URxvt*inheritPixmap: true
URxvt.transparent: true
URxvt.shading: 45
URxvt.Geometry: 120x96
URxvt.tintColor: #6a6a6a
URxvt.matcher.button: 		1


